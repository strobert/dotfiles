alias dlook 'grep ^\!* /usr/share/dict/words'
alias mkpasswd 'mkpasswd -s 0'
alias icontrol 'tar -xzf \!* -O ./control.tar.gz | tar -xvzf - ./control -O'
alias idatlist 'tar -xzf \!* -O ./data.tar.gz | tar -tvzf -'
