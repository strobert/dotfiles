# .tcshrc
setenv PATH /usr/sbin:/sbin:/usr/local/bin:/usr/bin:/bin:/home/strobert/bin:/usr/X11R6/bin:/usr/local/jdk/bin
setenv LS_COLORS di=36
setenv CLASSPATH /home/strobert/devel/java-out:/home/strobert/devel/ext-hack:.

set prompt="[%B%m%b] %n %t %d %~>"
set mail="/var/spool/mail/strobert"
set history=1000
set savehist=(3000 merge)
set histlit
set ignoreeof=3
unset autologout
set autolist=ambiguous
set implicitcd
set pushdtohome
set nobeep
if ($?loginsh) then
   setterm -blength 0
endif

source ~strobert/.alias

complete cvs p/1/"(add admin annotate checkout commit diff edit \
                   editors export history import init log login \
                   rdiff release remove rtag status tag unedit \
                   update watch)"/
